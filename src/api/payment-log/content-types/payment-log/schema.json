{
  "kind": "collectionType",
  "collectionName": "payment_logs",
  "info": {
    "singularName": "payment-log",
    "pluralName": "payment-logs",
    "displayName": "Payment Log",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "rz_order_creationId": {
      "type": "string"
    },
    "rz_payment_id": {
      "type": "string"
    },
    "amount": {
      "type": "float"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "AUTHORIZED",
        "CAPTURED",
        "FAILED"
      ]
    },
    "currency": {
      "type": "string",
      "default": "INR"
    },
    "method": {
      "type": "string"
    },
    "vpa": {
      "type": "string"
    },
    "account_number": {
      "type": "string"
    },
    "ifsc_code": {
      "type": "string"
    },
    "card_network": {
      "type": "string"
    },
    "card_id": {
      "type": "string"
    },
    "card_number": {
      "type": "string"
    },
    "card_type": {
      "type": "string"
    },
    "bank": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "contact": {
      "type": "string"
    },
    "order_id": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "payment_logs"
    },
    "settlement": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::settlement.settlement",
      "inversedBy": "payment_logs"
    },
    "settled": {
      "type": "boolean",
      "default": false
    },
    "payment_mode": {
      "type": "enumeration",
      "enum": [
        "COD",
        "PREPAID"
      ]
    },
    "payout_required": {
      "type": "boolean"
    },
    "payout_amount": {
      "type": "decimal"
    },
    "payout_log": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::payout-log.payout-log",
      "inversedBy": "payment_log"
    }
  }
}
